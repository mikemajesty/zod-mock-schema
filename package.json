{
  "name": "@mikemajesty/zod-mock-schema",
  "version": "1.0.14",
  "description": "Generate mock data from Zod schemas",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "keywords": [
    "zod",
    "mock",
    "testing",
    "faker",
    "schema",
    "typescript",
    "type-safe",
    "brazilian",
    "cpf",
    "cnpj",
    "brazil",
    "fixtures",
    "test-data",
    "mock-generator"
  ],
  "author": "Mike Lima",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "randexp": "^0.5.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:cjs-package",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:cjs-package": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "dev": "tsc --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikemajesty/zod-mock-schema.git"
  },
  "bugs": {
    "url": "https://github.com/mikemajesty/zod-mock-schema/issues"
  },
  "homepage": "https://github.com/mikemajesty/zod-mock-schema#readme"
}
